{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "arrow-parens": false,
    "member-access": [
      true,
      "no-public"
    ],
    "no-unused-expression": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ]
  },
  "rulesDirectory": []
}