{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": [false],
    "arrow-parens": false,
    "semicolon": [
      true,
      "never"
    ],
    "no-trailing-whitespace": false,
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "instance-field",
          "public-static-method",
          "public-instance-method",
          "private-static-method",
          "private-instance-method"
        ]
      }
    ]
  },
  "rulesDirectory": []
}