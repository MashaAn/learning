{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-trailing-whitespace": false,
    "quotemark": [
      true,
      "double"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "arrow-parens": false
  },
  "rulesDirectory": []
}